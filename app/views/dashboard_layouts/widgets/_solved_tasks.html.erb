<div class="widget border-bottom-none">
    <h3 class="innerAll handle bg-solved border-bottom margin-bottom-none"><%= t('reports.comparison.solved_tasks')%></h3>
    <a href="#" class="chart-toggle" data-chart-id="chart_horizontal_bars2">
        <span class="glyphicon glyphicon-plus"></span>
    </a>
    <div class="widget-body">
        <div id="chart_stacked_bars_tasks" class="flotchart-holder"></div>
    </div>
</div>
<script>
    $('#chart_stacked_bars_tasks').extremiaFlotchart(
            'comparisonReportChart',
            {
                'rawData': [
                    {
                        label: I18n.t('reports.comparison.solved_tasks_by_user'),
                        data: Report.tasksByUserData
                    }
                ],
                'rawTicks': Report.tasksByUserTicks,
                'render': true
            }
    );
</script>
