<%= simple_form_for(@fonnecta, url: super_ui_credentials_fonnecta_path, method: :put) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :client_id, autocomplete: 'off' %>
    <%= f.input :client_secret, autocomplete: 'off' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, 'Update' %>
  </div>
<% end %>
